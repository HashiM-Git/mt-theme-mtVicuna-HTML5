<MTSetVars>
	page_title=<$MTArchiveTitle$> Archive
	body=0
	<MTIf name="archive_class" like="yearly-arc">
		body=1
		ms=0
	</MTIf>
	tempName=index_archive_datebased
</MTSetVars><$MTInclude module="<__trans phrase="All Setting">"$>
<!DOCTYPE html>
<html lang="<$MTDefaultLanguage$>">
  <head>
    <meta charset="<$MTPublishCharset$>">
    <$MTInclude module="<__trans phrase="HTML Header">"$>
    <link rel="canonical" href="<$MTArchiveLink$>">
<MTIf name="datebased_archive">
<MTArchivePrevious>    <link rel="prev" href="<$MTArchiveLink$>" title="<$MTArchiveTitle$>"></MTArchivePrevious>
<MTArchiveNext>    <link rel="next" href="<$MTArchiveLink$>" title="<$MTArchiveTitle$>"></MTArchiveNext>
</MTIf>
<$MTCCLicenseRDF$>
    <title><$MTBlogName encode_html="1"$> - <$MTVar name="page_title"$></title>
    <$MTInclude module="<__trans phrase="Google Analytics">"$>
  </head>
  <body class="dateBasedArchive<MTIf name="body"> archives</MTIf> <$MTVar name="page_column"$><$MTVar name="eyecatch"$>">
    <$MTInclude module="<__trans phrase="Header">"$>
    <div id="content">
      <div id="main">
        <$MTInclude module="<__trans phrase="Topic Path">"$>
        <$MTInclude module="<__trans phrase="Next Entry Link">" sim="1"$>
        <h1><$MTVar name="page_title"$></h1>
        <div class="entry"><$MTEntriesCount setvar="limit"$>
<MTEntries sort_order="$entry" limit="$limit">
	<MTIf name="body">
		<MTIf name="M_groop">
			<$MTSetVar name="month2" value="0"$>
			<MTUnless name="ms">
				<$MTEntryDate format="%m" setvar="month"$>
          <section class="section">
            <h2><a href="./<$MTVar name="month"$>/" title="<$MTEntryDate format="%B"$><__trans phrase="Month">"><$MTEntryDate format="%b" language="en"$>.</a></h2><$MTSetVar name="ms" value="1"$>
			</MTUnless>
            <$MTInclude module="<__trans phrase="Headline">" ettl="h3"$>
			<$MTSetVar name="ms" op="++"$>
			<MTIf name="entry" eq="ascend">
				<MTEntryNext><$MTEntryDate format="%m" setvar="month2"$></MTEntryNext>
			<MTElse>
				<MTEntryPrevious><$MTEntryDate format="%m" setvar="month2"$></MTEntryPrevious>
			</MTIf>
			<MTIf name="month" ne="$month2">
          </section><$MTSetVar name="ms" value="0"$>
			</MTIf>
		<MTElse>
          <$MTInclude module="<__trans phrase="Headline">" ettl="h2"$>
		</MTIf>
	<MTElse>
		<MTIf name="entry_sh">
          <$MTInclude module="<__trans phrase="Headline">" ettl="h2"$>
		<MTElse>
          <$MTInclude module="<__trans phrase="Entry Summary">"$>
		</MTIf>
	</MTIf>
</MTEntries>
          <$MTInclude module="<__trans phrase="Next Entry Link">"$>
        </div>
        <$MTInclude module="<__trans phrase="Topic Path">"$>
      </div>
      <!-- end div#main -->
      <$MTInclude module="<__trans phrase="Utilities">"$>
    </div>
    <!-- end div#content -->
    <$MTInclude module="<__trans phrase="Footer">"$>
  </body>
</html>
