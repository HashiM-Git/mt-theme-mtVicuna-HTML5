<MTSetVars>
	tempName=index_archive_datebased
	page_title=<$MTArchiveTitle$> Archive
	blog_id=<$MTBlogID$>
	<MTIf name="archive_class" like="yearly-arc">
		body=1
		entry=ascend
		page_column=single
		ms=0
	<MTElse>
		body=0
		entry=descend
		page_column=double
	</MTIf>
</MTSetVars>
<!DOCTYPE html>
<html lang="<$MTDefaultLanguage$>">
  <head>
    <$MTInclude module="<__trans phrase="HTML Header">"$>
<MTIf name="datebased_archive">
<MTArchivePrevious>    <link rel="prev" href="<$MTArchiveLink$>" title="<$MTArchiveTitle$>"></MTArchivePrevious>
<MTArchiveNext>    <link rel="next" href="<$MTArchiveLink$>" title="<$MTArchiveTitle$>"></MTArchiveNext>
</MTIf>
    <title><$MTGetVar name="page_title"$> - <$MTBlogName encode_html="1"$></title>
  </head>
  <body class="dateBasedArchive<MTIf name="body"> archives</MTIf> <$MTGetVar name="page_column"$><$MTGetVar name="eyecatch"$>">
    <$MTInclude module="<__trans phrase="Header">"$>
    <div id="content">
      <div id="main">
        <$MTInclude module="<__trans phrase="Topic Path">"$>
        <$MTInclude module="<__trans phrase="Next Entry Link">" sim="1"$>
        <h1><$MTGetVar name="page_title"$></h1>
        <div class="entry">
<MTEntries sort_order="$entry">
	<MTUnless name="body">
          <$MTInclude module="<__trans phrase="Entry Summary">"$>
	<MTElse>
		<$MTSetVar name="month2" value="0"$>
		<MTUnless name="ms">
			<$MTEntryDate format="%m" setvar="month"$>
          <section class="section">
            <h1><a href="./<$MtEntryDate format="%m"$>/" title="<$MTEntryDate format="%B"$><__trans phrase="Month">"><$MTEntryDate format="%b" language="en"$>.</a></h1><$MTSetVar name="ms" value="1"$>
		</MTUnless>
            <$MTInclude module="<__trans phrase="Headline">" ettl="h2"$>
		<$MTSetVar name="ms" op="+" value="1"$>
		<MTEntryNext><$MTEntryDate format="%m" setvar="month2"$></MTEntryNext>
		<MTIf name="month" ne="$month2">
          </section><$MTSetVar name="ms" value="0"$>
		</MTIf>
	</MTUnless>
</MTEntries>
          <$MTInclude module="<__trans phrase="Next Entry Link">" sim="0"$>
        </div>
        <$MTInclude module="<__trans phrase="Topic Path">"$>
      </div>
      <!-- end div#main -->
      <$MTInclude module="<__trans phrase="Utilities">"$>
      <$MTInclude module="<__trans phrase="Return to page top">"$>
    </div>
    <!-- end div#content -->
    <$MTInclude module="<__trans phrase="Footer">"$>
  </body>
</html>
