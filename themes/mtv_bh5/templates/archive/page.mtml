<MTSetVars>
	page_column=single
	page_title=<$MTPageTitle remove_html="1"$>
	current_page=<$MTPageID$>
	tempName=individual_webpage
</MTSetVars><$MTInclude module="<__trans phrase="All Setting">"$>
<!DOCTYPE html>
<html lang="<$MTDefaultLanguage$>">
  <head<MTIf name="fbadmin"> prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#"</MTIf>>
    <meta charset="<$MTPublishCharset$>"><MTIf name="fbadmin">
    <meta property="og:title" content="<$MTBlogName encode_html="1"$>: <$MTGetVar name="page_title"$>">
    <meta property="og:type" content="article">
    <meta property="og:url" content="<$MTPagePermalink$>">
<MTIgnore><meta property="og:image" content="Eye catch picture of a page"></MTIgnore>
    <meta property="og:site_name" content="<$MTBlogName encode_html="1"$>">
    <meta property="og:locale" content="ja_JP">
    <meta property="og:description" content="<$MTPageExcerpt remove_html="1" ltrim="1"$>">
    <meta property="fb:admins" content="<$MTGetVar name="fbadmin"$>">
	<MTIf name="bu_arrey" index="0">
    <meta property="fb:app_id" content="<$MTGetVar name="bu_arrey" index="0"$>"></MTIf></MTIf>
    <meta name="description" content="<$MTPageExcerpt remove_html="1" ltrim="1"$>">
<MTIf tag="PageKeywords">    <meta name="keywords" content="<$MTPageKeywords remove_html="1"$>"></MTIf>
    <$MTInclude module="<__trans phrase="HTML Header">"$>
    <link rel="canonical" href="<$MTPagePermalink$>">
<$MTCCLicenseRDF$>
<MTIfPingsAccepted><$MTEntryTrackbackData$></MTIfPingsAccepted>
    <title><$MTBlogName encode_html="1"$> - <$MTGetVar name="page_title"$></title>
    <$MTInclude module="<__trans phrase="Google Analytics">"$>
  </head>
  <body class="webPage <$MTGetVar name="page_column"$><$MTGetVar name="eyecatch"$>">
    <$MTInclude module="<__trans phrase="Header">"$>
    <div id="content">
      <div id="main">
        <$MTInclude module="<__trans phrase="Topic Path">"$>
        <h1><$MTGetVar name="page_title"$></h1>
        <article class="entry">
          <header>
            <ul class="info">
              <li class="date">Last Modified: <time datetime="<$MTPageModifiedDate format_name="iso8601"$>"><$MTPageModifiedDate format="%x %X"$></time></li>
            </ul>
          </header>
<MTIf tag="PageBody">
          <div class="textBody">
<$MTPageBody$>
          </div>
</MTIf>
<MTIf tag="PageMore">
          <div class="textBody" id="extended">
<$MTPageMore$>
          </div>
</MTIf>
          <$MTInclude module="<__trans phrase="Comments">"$>
          <$MTInclude module="<__trans phrase="TrackBacks">"$>
        </article>
        <!-- end article.entry -->
        <$MTInclude module="<__trans phrase="Topic Path">"$>
      </div>
      <!-- end div#main -->
      <$MTInclude module="<__trans phrase="Utilities">"$>
    </div>
    <!-- end div#content -->
    <$MTInclude module="<__trans phrase="Footer">"$>
  </body>
</html>
