<MTIf tag="AssetCount">
          <section>
            <h3>Photos</h3>
            <div class="recentAssets">
              <ul class="thumb">
	<MTAssets type="image" lastn="5">
                <li><a href="<$MTAssetURL$>" rel="nofollow"><img src="<$MTAssetThumbnailURL height="70" square="1"$>" width="70" height="70" alt="<$MTAssetLabel$>"></a></li>
	</MTAssets>
              </ul>
            </div>
          </section>
</MTIf>
