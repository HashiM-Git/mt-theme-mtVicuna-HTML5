<MTIf tag="AssetCount">
          <section>
            <h1>Photos</h1>
            <div class="recentAssets">
              <ul class="thumb">
	<MTAssets type="image">
                <li><a href="<$MTAssetURL$>" rel="nofollow"><img src="<$MTAssetThumbnailURL height="70"$>" alt="<$MTAssetLabel$>"></a></li>
	</MTAssets>
              </ul>
            </div>
          </section>
</MTIf>
