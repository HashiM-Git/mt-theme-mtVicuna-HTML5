<MTTags>
	<MTIf name="__first__">
          <section>
            <h3>Tag Cloud</h3>
            <div class="tagCloud">
              <ul class="tagCloud">
	</MTIf>
                <li class="level<$MTTagRank$>"><a href="<$MTTagSearchLink$>" title="<$MTTagCount$> Entries" rel="nofollow"><$MTTagName$></a></li>
	<MTIf name="__last__">
              </ul>
            </div>
          </section>
	</MTIf>
</MTTags>
